    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "tsBuildInfoFile": "../../../dist/libs/shared/binary-bridge/tsconfig.tsbuildinfo"
  },
  "files": [],
  "include": [],
  "references": [
    { "path": "./tsconfig.lib.json" }
  ]
